{
    "name": "design-docs",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "vite",
        "preview": "vite preview",
        "build": "vite build && rm -rf ../../docs && mkdir ../../docs && cp -R dist/ ../../docs && cp ../../CNAME ../../docs && cp ../../.nojekyll ../../docs",
        "test": "vitest run",
        "test:dev": "vitest watch",
        "test:coverage": "vitest run --coverage",
        "lint": "eslint src/**/*.{ts,vue}",
        "lint:fix": "eslint --fix src/**/*.{ts,vue}",
        "format": "prettier -c 'src/**/*.{ts,vue,scss,spec.ts}'",
        "format:fix": "prettier --write 'src/**/*.{ts,vue,scss,spec.ts}'"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vitest/coverage-c8": "^0.33.0",
        "@vue/eslint-config-prettier": "^7.1.0",
        "@vue/eslint-config-typescript": "^11.0.3",
        "@vue/test-utils": "^2.4.0",
        "eslint": "^8.44.0",
        "eslint-plugin-prettier": "alpha",
        "eslint-plugin-vue": "^9.15.1",
        "highlight.js": "^11.8.0",
        "prettier": "^3.0.0",
        "protean-elements": "^0.0.1",
        "protean-styles": "^0.0.1",
        "sass": "^1.63.6",
        "typescript": "^5.1.6",
        "vite": "^4.4.2",
        "vitest": "^0.33.0",
        "vue": "^3.3.4",
        "vue-router": "^4.2.4"
    }
}
