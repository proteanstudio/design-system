{
    "name": "design-docs",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "vue-cli-service serve",
        "build": "vue-cli-service build --modern && rm -rf ../../docs && mkdir ../../docs && cp -R dist/ ../../docs && cp ../../CNAME ../../docs",
        "test": "vue-cli-service test:unit",
        "test:dev": "vue-cli-service test:unit --watch",
        "lint": "vue-cli-service lint",
        "format": "prettier -c 'src/**/*.{ts,vue,scss,spec.ts}'",
        "format:fix": "prettier --write 'src/**/*.{ts,vue,scss,spec.ts}'"
    },
    "devDependencies": {
        "@types/highlight.js": "^10.1.0",
        "@types/jest": "^27.0.3",
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.48.2",
        "@vue/cli-plugin-babel": "~5.0.8",
        "@vue/cli-plugin-eslint": "~5.0.8",
        "@vue/cli-plugin-router": "~5.0.8",
        "@vue/cli-plugin-typescript": "~5.0.8",
        "@vue/cli-plugin-unit-jest": "~5.0.8",
        "@vue/cli-service": "~5.0.8",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "@vue/test-utils": "^2.2.7",
        "core-js": "^3.27.2",
        "eslint": "^8.32.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.9.0",
        "highlight.js": "^11.7.0",
        "prettier": "^2.8.3",
        "protean-elements": "^0.0.1",
        "protean-styles": "^0.0.1",
        "sass": "^1.57.1",
        "sass-loader": "^13.2.0",
        "typescript": "^4.9.4",
        "vue": "^3.2.45",
        "vue-class-component": "^8.0.0-0",
        "vue-jest": "^5.0.0-0",
        "vue-property-decorator": "^9.1.2",
        "vue-router": "^4.1.6"
    }
