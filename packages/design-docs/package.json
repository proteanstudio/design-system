{
    "name": "design-docs",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "vite",
        "preview": "vite preview",
        "build": "vite build && rm -rf ../../docs && mkdir ../../docs && cp -R dist/ ../../docs && cp ../../CNAME ../../docs",
        "test": "vitest run",
        "test:dev": "vitest watch",
        "test:coverage": "vitest run --coverage",
        "lint": "eslint",
        "lint:fix": "eslint --fix src/",
        "format": "prettier -c 'src/**/*.{ts,vue,scss,spec.ts}'",
        "format:fix": "prettier --write 'src/**/*.{ts,vue,scss,spec.ts}'"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.48.2",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vitest/coverage-c8": "^0.28.1",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "@vue/test-utils": "^2.2.7",
        "eslint": "^8.32.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.9.0",
        "highlight.js": "^11.7.0",
        "jest": "^29.3.1",
        "prettier": "^2.8.3",
        "protean-elements": "^0.0.1",
        "protean-styles": "^0.0.1",
        "sass": "^1.57.1",
        "typescript": "^4.9.4",
        "vite": "^4.0.4",
        "vitest": "^0.27.2",
        "vue": "^3.2.45",
        "vue-class-component": "^8.0.0-rc.1",
        "vue-property-decorator": "^10.0.0-rc.3",
        "vue-router": "^4.1.6"
    }
}
