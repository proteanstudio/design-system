{
    "name": "design-docs",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "vite",
        "preview": "vite preview",
        "build": "vite build && rm -rf ../../docs && mkdir ../../docs && cp -R dist/ ../../docs && cp ../../CNAME ../../docs",
        "test": "vitest run",
        "test:dev": "vitest watch",
        "test:coverage": "vitest run --coverage",
        "lint": "eslint",
        "lint:fix": "eslint --fix src/",
        "format": "prettier -c 'src/**/*.{ts,vue,scss,spec.ts}'",
        "format:fix": "prettier --write 'src/**/*.{ts,vue,scss,spec.ts}'"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vitest/coverage-c8": "^0.28.2",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "@vue/test-utils": "^2.2.7",
        "eslint": "^8.33.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.9.0",
        "highlight.js": "^11.7.0",
        "prettier": "^2.8.3",
        "protean-elements": "^0.0.1",
        "protean-styles": "^0.0.1",
        "sass": "^1.58.0",
        "typescript": "^4.9.5",
        "vite": "^4.1.1",
        "vitest": "^0.28.4",
        "vue": "^3.2.47",
        "vue-router": "^4.1.6"
    }
}
